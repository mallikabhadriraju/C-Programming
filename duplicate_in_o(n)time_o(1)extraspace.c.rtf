void printRepeating(int arr[], int size)\
\{\
  int i;\
  printf("The repeating elements are: \\n");\
  for (i = 0; i < size; i++)\
  \{\
    if (arr[abs(arr[i])] >= 0)\
      arr[abs(arr[i])] = -arr[abs(arr[i])];\
    else\
      printf(" %d ", abs(arr[i]));\
  \}\
\}\
