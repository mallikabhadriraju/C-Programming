int main()\
\{\
  int arr[] = \{1, 2, 3, 1, 3, 6, 6\};\
  int arr_size = sizeof(arr)/sizeof(arr[0]);\
  printRepeating(arr, arr_size);\
  getchar();\
  return 0;\
\}\
